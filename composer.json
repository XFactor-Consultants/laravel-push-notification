{
    "name": "xfactor/laravel-push-notification",
    "description": "Laravel package to send push notifications to mobile devices (apns, gcm)",
    "keywords": ["apns","gcm","push","notification", "laravel"],
    "authors": [
        {
            "name": "DaviBenNun",
            "email": "davi@andradenunes.org"
        },
        {
            "name": "Colin McNeil",
            "email": "cmcneil@xfactorconsultants.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*|^6.0|^7.0|^8.0",
        "sly/notification-pusher": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Davibennun\\LaravelPushNotification": "src/"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/turbo124/NotificationPusher"
        }
    ]
